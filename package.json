{
  "name": "react-nsn",
  "version": "0.1.0",
  "private": true,
  "description": "A very lightweight and customizable online network status notification built for react apps.",
  "keywords": [
    "react",
    "notification",
    "react-nsn",
    "status",
    "online-status",
    "network-status",
    "online-status-notification",
    "network-notification",
    "network-notifier",
    "javascript",
    "react-component"
  ],
  "author": "AmrAhmed <geek3mr@gmail.com> (http://github.com/AmrAhmedA)",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmrAhmedA/react-nsn"
  },
  "source": "src/nsn.ts",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "start": "cd example && npm run start",
    "prebuild": "rm -rf dist/",
    "build": "parcel build",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install",
    "format": "prettier --write src",
    "pre-commit": "lint-staged",
    "clean": "rimraf dist && rimraf --glob node_modules"
  },
  "lint-staged": {
    "*.@(js|json|ts|tsx)": [
      "npm run format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.8.3",
    "@parcel/transformer-webmanifest": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "process": "^0.11.10",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
